{
  "ignorePatterns": [
    {
      "pattern": "{link}"
    },
    {
      "pattern": "twitter"
    },
    {
      "pattern": "http://localhost"
    },
    {
      "pattern": "https://www.greencarcongress.com"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://docs.github.com/"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/images/",
      "replacement": "{{BASEURL}}/static/images/"
    },
    {
      "pattern": "^/([a-zA-Z._-]+)$",
      "replacement": "{{BASEURL}}/src/pages/$1.md"
    }
  ],
  "timeout": "5s"
}
